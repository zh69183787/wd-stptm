<!DOCTYPE struts PUBLIC    
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"    
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="DbBusiness" namespace="/dbBusiness" extends="cuteframework-default">
		
		<action name="addDbBusiness" class="" method="addDbBusiness"/>
		<action name="deleteDbBusiness" class="" method="deleteDbBusiness"/>
		<action name="findDbBusinessById" class="" method="findDbBusinessById"/>
		<action name="updateDbBusiness" class="" method="updateDbBusiness"/>

		<action name="findDbBusinessByPage" class="dbBusinessAction" method="findDbBusinessByPage">
			<result>/dbBusiness/dbBusinessList.jsp</result>
			<result name="count">/dbBusiness/countByDept.jsp</result>
		</action>
		
		<action name="toDbBusinessView" class="dbBusinessAction" method="toDbBusinessView">
			<result>/dbBusiness/viewDbBusiness.jsp</result>
		</action>
		<action name="deleteDbBusiness" class="dbBusinessAction" method="deleteDbBusiness"/>
		
	</package>
</struts>
	
